function[x]=Inverse(L,D,b)
%This function solve a lower triangular system.
b = [1 3 4 3]';
y = L\b;
x = (D*L')\y;
m=S(1);
if S(1)~=S(2)
    error('matrix mast be square')
end
x=zeros(1,m);
x(1,1)=b(1)./L(1,1);
%bacward substitution
for k=2:m
   
  
        x1=1/L(k,k).*(b(k)-sum(L(k,k-1:-1:1).*x(k-1:-1:1)));
        x(1,k)=x1;
end
x=x';
end